{
  "name": "@sambit24795/freelance-gigs-shared",
  "version": "2.0.1",
  "license": "MIT",
  "author": "sambit nayak",
  "description": "Helpers library for Jobber app",
  "engines": {
    "node": ">=20"
  },
  "main": "build/cjs/index.js",
  "module": "build/esm/index.js",
  "types": "build/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./build/src/index.d.ts",
      "import": "./build/esm/index.js",
      "require": "./build/cjs/index.js"
    }
  },
  "files": [
    "build/cjs",
    "build/esm",
    "build/src",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && tsc --outDir build/src",
    "build:cjs": "cross-env NODE_ENV=production BABEL_ENV=cjs babel src --presets=./scripts/babel-preset.js --extensions .ts,.tsx --ignore src/**/*.specs.tsx --out-dir build/cjs --source-maps",
    "build:esm": "cross-env NODE_ENV=production BABEL_ENV=esm babel src --presets=./scripts/babel-preset.js --extensions .ts,.tsx --ignore src/**/*.specs.tsx --out-dir build/esm --source-maps",
    "prepack": "npm run build"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sambit24795/freelance-gigs-shared.git"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^9.1.0",
    "cloudinary": "^2.7.0",
    "express": "^5.1.0",
    "http-status-codes": "^2.3.0",
    "mongoose": "^8.17.0",
    "typescript": "^5.9.2",
    "winston": "^3.17.0",
    "winston-elasticsearch": "^0.19.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-typescript": "^7.27.1",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "cross-env": "^10.0.0"
  }
}
